tsx",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\components\\layout\\header\\navigation\\Navigation.tsx",["128","129"],"import React, {useContext, useState} from 'react';\nimport {Link, NavLink} from 'react-router-dom';\nimport {Container, Nav, Navbar, NavbarBrand} from 'react-bootstrap'\nimport {routes} from 'constants/routes'\nimport 'components/layout/header/navigation/Navigation.scss'\nimport AppContext from 'context/app';\nimport UserLogout from 'components/user/logout/Logout';\nimport drivingLicenseLogo from 'assets/images/icons/driving-license.svg';\nimport Red_Cross from 'assets/images/icons/Red_Cross.png';\nimport companyNameLogo from 'assets/images/icons/company-name.png';\n\n/**\n * Stateful component responsible for rendering the top navigation of this application.\n * There are 2 main navigation blocks: for authenticated and non-authenticated user.\n * Non-authenticated user will be able to see links to \"Register\" and \"Login\", while\n * authenticated user will be able to see the \"Logout\" link and his username.\n * */\nconst LayoutHeaderNavigation = () => {\n  const [showNavbar, setShowNavbar] = useState<boolean>(false);\n  const {appState} = useContext(AppContext)\n\n  // In order to fix the \"findDOMNode\" warning in the navbar collapse component,\n  // a custom toggle control function was implemented. Currently, some bootstrap components\n  // are not up-to-date with React standards, and this is considered a quick-fix until\n  // bootstrap components are updated.\n  return (\n    <Navbar expand=\"lg\"\n            onToggle={() => setShowNavbar(!showNavbar)}\n            expanded={false}\n            className='bg-orange'\n    >\n      <Container>\n        <NavbarBrand>\n          <Link to={routes.ROOT}>\n            <img src={Red_Cross} className='logo-icon' alt='logo'/>\n            {/* <img src={companyNameLogo} className='logo-name' alt='logo-name'/> */}\n            <span className=\"font-weight-bold text-dark px-2\">Quality Assurance Agency of China (Issuer)</span>\n          </Link>\n        </NavbarBrand>\n        <Navbar.Toggle aria-controls=\"basic-navbar-nav\" />\n        <Navbar.Collapse className={showNavbar ? 'show' : ''}>\n          <Nav className=\"justify-content-end\">\n            {appState.isAuthenticated && (\n              <>\n                <Nav.Item>\n                  <Nav.Link as={'span'} className='user-logout'>\n                    <UserLogout />\n                  </Nav.Link>\n                </Nav.Item>\n                <Nav.Item>\n                  <Nav.Link as={'span'}>\n                    <span>| Hello, {appState.username}</span>\n                  </Nav.Link>\n                </Nav.Item>\n              </>\n            )}\n\n            {!appState.isAuthenticated && (\n              <>\n                <Nav.Item>\n                  <Nav.Link as={NavLink} activeClassName='is-active' to={routes.APPLICANT_LOGIN}>Start applying</Nav.Link>\n                </Nav.Item>\n                <Nav.Item>\n                  <Nav.Link as={NavLink} activeClassName='is-active' to={routes.ISSUER_LOGIN}>Sign in as admin</Nav.Link>\n                </Nav.Item>\n              </>\n            )}\n          </Nav>\n        </Navbar.Collapse>\n      </Container>\n    </Navbar>\n  )\n}\n\nexport default LayoutHeaderNavigation\n","D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\utils\\config.ts",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\constants\\endpoints.ts",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\constants\\localstorage.ts",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\constants\\routes.ts",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\pages\\not-found\\NotFound.tsx",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\pages\\application\\Application.tsx",["130"],"import React, { useState, useContext } from 'react';\nimport AppContext from 'context/app';\nimport {Button, FormControl, FormGroup, FormLabel, FormFile} from 'react-bootstrap';\nimport ApiService from 'utils/apiService';\nimport 'pages/application/Application.scss'\nimport firebase from 'utils/firebase/firebase';\nimport randomstring from 'randomstring';\n\ninterface IBaseVCData {\n    givenName: string;\n    familyName: string;\n    address: string;\n}\n  \ninterface IExtendVCData {\n  uen: string;\n  email: string;\n  dateOfIncorporation: string;\n  companyType: string;\n  principalBusinessActivity: string;\n  productName: string\n}\n\nconst defaultBaseVCData: IBaseVCData = {\n  givenName: '',\n  familyName: '',\n  address: ''\n}\n\nconst defaultExtendVCData: IExtendVCData = {\n  uen: '',\n  email: '',\n  dateOfIncorporation: '',\n  companyType: '',\n  principalBusinessActivity: '',\n  productName: ''\n}\n\ninterface IPayload extends IBaseVCData{\n  idClass: string;\n  holderDid: string\n}\n\nconst Application: React.FC = (): React.ReactElement => {\n    const {appState} = useContext(AppContext);\n    const [inputDID, setinputDID] = useState(appState.didToken || '')\n\n    const [baseVCData, setBaseVCData] = useState<IBaseVCData>(defaultBaseVCData)\n  \n    const [extendVCData, setExtendVCData] = useState<IExtendVCData>(defaultExtendVCData)\n\n    /**\n     * Function for issuing an unsigned employment VC.\n     * */\n    const submitApplication = async () => {\n        try {\n          const { givenName, familyName, address } = baseVCData;\n\n          // Generate a random Affinidi Driving License ID, which will double up as an application ID\n          const applicationID: string = randomstring.generate(10);\n          const vcToStringify = {...extendVCData, applicationID}\n          \n          const payload: IPayload = {\n            givenName,\n            familyName,\n            address,\n            idClass: JSON.stringify(vcToStringify),\n            holderDid: inputDID || appState.didToken || '',\n          }\n\n          // Store unsignedVC into issuer's datsabase\n          const db = firebase.firestore();\n          db.collection('application-waiting-approval').add({username: appState.username, payload, applicationID, approved: false})\n\n          alert('You have successfully submitted your application.');\n        } catch (error) {\n            ApiService.alertWithBrowserConsole(error.message);\n        }\n    }\n    \n    const resetToDefaults = () => {\n      setinputDID(appState.didToken || '')\n\n      setBaseVCData(defaultBaseVCData)\n      setExtendVCData(defaultExtendVCData)\n    }\n    \n    const updateBaseVC = (e: any) => {\n      setBaseVCData({...baseVCData, [e.target.name]: e.target.value})\n    }\n\n    const updateExtendBaseVC = (e: any) => {\n      setExtendVCData({...extendVCData, [e.target.name]: e.target.value})\n    }\n\n    return (\n      <div className='tutorial'>\n        <div className='tutorial__step'>\n          <Button \n            style={{float: 'right'}}\n            onClick={e => resetToDefaults()}\n            >Clear all fields\n          </Button>\n\n          <p><strong>Step 1:</strong>Please fill in details of your company</p>\n          <FormGroup controlId='email'>\n            <FormLabel className='label' style={{margin: '10px 0 0 0'}}>Email Address:</FormLabel>\n            <FormControl name='email' type='text' value={extendVCData.email} onChange={e => updateExtendBaseVC(e)}/>\n          </FormGroup>\n\n          <FormGroup controlId='givenName'>\n            <FormLabel className='label' style={{margin: '10px 0 0 0'}}>Company Name:</FormLabel>\n            <FormControl name='givenName' type='text' value={baseVCData.givenName} onChange={e => updateBaseVC(e)}/>\n          </FormGroup>\n\n          <FormGroup controlId='address'>\n            <FormLabel style={{margin: '10px 0 0 0'}}>Address:</FormLabel>\n            <FormControl name='address' type='text' value={baseVCData.address} onChange={e => updateBaseVC(e)}/>\n          </FormGroup>\n\n          <FormGroup controlId='uen'>\n            <FormLabel style={{margin: '10px 0 0 0'}}>Unique Entity Number (UEN):</FormLabel>\n            <FormControl name='uen' type='text' value={extendVCData.uen} onChange={e => updateExtendBaseVC(e)}/>\n          </FormGroup>\n\n          <FormGroup controlId='dateOfIncorporation'>\n            <FormLabel style={{margin: '10px 0 0 0'}}>Date of Incorporation:</FormLabel>\n            <FormControl name='dateOfIncorporation' type='text' value={extendVCData.dateOfIncorporation} onChange={e => updateExtendBaseVC(e)}/>\n          </FormGroup>\n\n          <FormGroup controlId='companyType'>\n            <FormLabel style={{margin: '10px 0 0 0'}}>Company Type:</FormLabel>\n            <FormControl name='companyType' type='text' value={extendVCData.companyType} onChange={e => updateExtendBaseVC(e)}/>\n          </FormGroup>\n\n          <FormGroup controlId='activity'>\n            <FormLabel style={{margin: '10px 0 0 0'}}>Principal Business Activity:</FormLabel>\n            <FormControl name='principalBusinessActivity' type='text' value={extendVCData.principalBusinessActivity} onChange={e => updateExtendBaseVC(e)}/>\n          </FormGroup>\n          \n          <FormGroup controlId='productName'>\n            <FormLabel style={{margin: '10px 0 0 0'}}>Product Name:</FormLabel>\n            <FormControl name='productName' type='text' value={extendVCData.productName} onChange={e => updateExtendBaseVC(e)}/>\n          </FormGroup>\n\n          <Button \n            onClick={e => submitApplication()}\n            >Submit\n          </Button>\n        </div>\n      </div>\n    )\n}\n\nexport default Application;","D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\pages\\api-key\\ApiKey.tsx",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\pages\\issuer\\Issuer.tsx",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\pages\\issuer\\ApplicationInfoPage.tsx",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\pages\\intro\\Intro.tsx",["131"],"import React from 'react';\nimport 'pages/intro/Intro.scss'\nimport EntireFlow from 'assets/images/icons/entire-flow.png';\nimport {routes} from 'constants/routes';\n\n/**\n * Stateless component responsible for rendering a simple SSI introduction screen.\n * */\nconst IntroPage = () => {\n  return (\n    <div className='intro page-form page-form--large'>\n      <div className='intro__heading-block'>\n        <h1 className='intro__heading'>\n          Quality Assurance for Healthcare Products \n        </h1>\n        <h5 className='intro__subheading'>A use case of VCs during times of world-wide pandemic</h5>\n      </div>\n      <div className='intro__text-block'>\n        <h4>Scenario</h4>\n        <p>The world has been unexpectedly struck by a pandemic and the global supply of masks is running dangerously low. Alex, a business man based in Shanghai who owns a mask manufacturing company in China, saw an opportunity to contribute to the humanitarian efforts of supplying healthcare resources for pandemic-stricken nations. On the business end, Alex also saw an opportunity to yield profits from the sale of masks and further establish a global presence to his company’s name. </p>\n        <p>Alex thought of exporting supplies of masks to Singapore, but Singapore has always been extremely strict on the surveyance of the quality of its imported goods. Alex needed to find a way to get his mask approved by a trusted local entity in China so that Singapore could recognize his company and trust his products.</p>\n        <p>On the other hand, Singapore is facing a major shortage of mask supplies as its larger partner healthcare companies in China, which Singapore had always depended on for its supply of masks, were grappling with the world-wide sudden surge in the demand for masks. Singapore did not mind turning to smaller mask manufacturers in China, such as Alex’s company, to procure masks from for its citizens. However, Singapore needed a way to verify the quality of the masks by the suppliers before importing them. Singapore needed to platform in which the suppliers listed have their products verified by a trusted entity so that import operations can commence immediately during dire times of a pandemic.</p>\n        <h4>Roles in this scenario</h4>\n        <p>There are 3 roles in SSI cycle: <strong>ISSUER</strong>, <strong>VERIFIER</strong>, and <strong>HOLDER</strong>. Each of them is explained in the example below.</p>\n      </div>\n      {/* <div className='intro__example'>\n        <img className='flow-size' src={EntireFlow} alt='entire-flow'/>\n      </div> */}\n      <div className='intro__roles-description'>\n        <div className='intro__roles-description-role'>\n          <h3>Issuer</h3>\n          <p>The Quality Assurance Agency of China is able to issue a digitised VC that has the approval information from its partner companies that they have engaged in to conduct checks on the products of the companies who requested for the VCs</p>\n        </div>\n        <div className='intro__roles-description-role'>\n          <h3>Holder</h3>\n          <p>Companies like Alex’s, which are able to provide immediate resources during times of pandemic or calamities, and would like a way of garnering trusts from their potential beneficiaries </p>\n        </div>\n        <div className='intro__roles-description-role'>\n          <h3>Verifier</h3>\n          <p>Can be a nation, a local distributor or an individual, who would like to be able to verify the VCs and not worry about the quality of the received products in times of emergencies. An example for this scenario would be the Temasek Foundation of Singapore</p>\n        </div>\n      </div>\n\n      <p>Ready to try out the application? Get started <a href={routes.APPLICANT_LOGIN} rel='noreferrer'>here</a> by being the applicant first!</p>\n      <p>For more information and well documented tutorials, please visit <a href='https://www.affinidi.com/api' target='_blank' rel='noreferrer'>https://www.affinidi.com/api</a>.</p>\n    </div>\n  )\n}\n\nexport default IntroPage\n","D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\pages\\user\\Issuer\\IssuerLogin.tsx",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\pages\\user\\Applicant\\Applicant.tsx",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\components\\user\\logout\\Logout.tsx",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\utils\\decodeEnv.ts",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\utils\\templates\\email.ts",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\utils\\vc-data-examples\\drivinglicense.ts",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\utils\\firebase\\firebase.ts",[],"D:\\Installations\\VS Projects\\affinidi-hackathon-chok\\affinidi-hackathon-chok\\affinidi-qualitycheck-issuer\\src\\components\\admin\\AdminList.tsx",[],"D:\\Installations\\VS 